\begin{document}


%---------- HEADING ----------

\begin{center}
  \textbf{\Huge \scshape <%= @user.cv_heading.full_name %>} \\ \vspace{3pt}
  \small
<% @user.cv_heading.cv_heading_items.each_with_index do |item, index| %>
<%   latex_icon = IconMapper.latex_icon_name(item.icon) || '' %>
<%   item_text = item.text.to_s %>
<%   item_url = item.url.presence %>
<%   is_last = (index == @user.cv_heading.cv_heading_items.length - 1) %>
<%   if item_url %>
<%     if latex_icon.present? %>
    <%= latex_icon %> \hspace{.5pt} \href{<%= item_url %>}{<%= item_text %>}
<%     else %>
    \href{<%= item_url %>}{<%= item_text %>}
<%     end %>
<%   else %>
<%     if latex_icon.present? %>
    <%= latex_icon %> \hspace{.5pt} <%= item_text %>
<%     else %>
    <%= item_text %>
<%     end %>
<%   end %>
<%   unless is_last %>
    $|$
<%   end %>
<% end %>
\end{center}




<% if educations.any? %>
%----------- EDUCATION -----------
\section{Education}
  \vspace{3pt}
  \resumeSubHeadingListStart
  <% educations.each do |edu| %>

    <% if !edu.additional_info.nil? && edu.additional_info != "" %>
    \resumeSubheading
    <% else %>
    \resumeEducationHeading
    <% end %>
      {<%= edu.institution %>}{<%= edu.location %>}
      {<%= edu.degree %> <%= "\\textbf{GPA: " + edu.gpa + "}" if edu.gpa != "" %>}{<%= format_date_range(edu.start_date, edu.end_date) %>}
    <% if !edu.additional_info.nil? && edu.additional_info != "" %>
        \resumeItemListStart
          \resumeItem{<%= edu.additional_info %>}
        \resumeItemListEnd
    <% else %>
      {}{} 
    <% end %>
  <% end %>

  \resumeSubHeadingListEnd
<% end %>

<% if experiences.any? %>
%----------- EXPERIENCE -----------
\section{Experience}
  \vspace{3pt}
  \resumeSubHeadingListStart
    <% experiences.each do |exp| %>

    \resumeSubheading
      {<%= exp.company %>}{<%= exp.location %>}
      {<%= exp.position_title %>}{<%= format_date_range(exp.start_date, exp.end_date) %>}
        <% if !exp.experience_bullets.nil? && !exp.experience_bullets.empty? %>
        \resumeItemListStart
            <% exp.experience_bullets.each do |b| %>
              \resumeItem{<%= b.content %>}
            <% end %>
        \resumeItemListEnd
      <% end %>
    <% end %>

  \resumeSubHeadingListEnd

<% end %>

<% if projects.any? %>
%----------- PROJECTS -----------

\section{Projects}
    \vspace{3pt}
    \resumeSubHeadingListStart
    <% projects.each do |prj| %>

      \resumeProjectHeading
        {\textbf{<%= prj.name %>} <%= '$|$  \emph{\href{'+prj.link+'}{\color{blue}'+(prj.link_title ? prj.link_title : prj.link) +'}}' if prj.link %>}{<%= prj.date %>}
        <% if !prj.project_bullets.nil? && !prj.project_bullets.empty? %>
        \resumeItemListStart
            <% prj.project_bullets.each do |b| %>
              \resumeItem{<%= escape_latex(b.content) %>}
            <% end %>
        \resumeItemListEnd
      <% end %>
    <% end %>

    \resumeSubHeadingListEnd
<% end %>

